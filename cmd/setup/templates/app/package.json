{
  "name": "{{.projectName}}",
  "version": "0.0.1",
  "description": "{{.description}}",
  "main": "src/Application.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev-server": "webpack-dev-server --config webpack.dev.config.js --hot --history-api-fallback",
    "test": "npx jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test-sonar": "npx jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test-ci": "npx jest --ci --reporters=default --reporters=jest-junit"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testSupport/setup.js"
    ],
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "^.+\\.(scss|css|svg|png|sass)$": "identity-obj-proxy"
    }
  },
  "author": "Pyramid Systems, Inc.",
  "license": "ISC",
  "dependencies": {
    "@pyramidlabs/react-ui": ">=0.0.39",
    "autoprefixer": "^9.4.2",
    "aws-sdk": "^2.407.0",
    "axios": "^0.18.0",
    "buffer": "^5.2.1",
    "node-sass": "^4.11.0",
    "promise-polyfill": "^8.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "postcss-cli": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.2.0",
    "@types/enzyme": "^3.1.18",
    "@types/jest": "^24.0.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-config": "^1.1.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-mock-store": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^2.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "jest-junit": "^6.2.1",
    "react-accessible-accordion": "^2.4.5",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
